<h1>Listing auths</h1>

<table>
  <tr>
    <th>Authid</th>
    <th>Name</th>
    <th>Provider</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @auths.each do |auth| %>
  <tr>
    <td><%= auth.authid %></td>
    <td><%= auth.name %></td>
    <td><%= auth.provider %></td>
    <td><%= link_to 'Show', auth %></td>
    <td><%= link_to 'Edit', edit_auth_path(auth) %></td>
    <td><%= link_to 'Destroy', auth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Auth', new_auth_path %>
