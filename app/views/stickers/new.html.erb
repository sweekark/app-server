<%= form_for(:sticker, :url => {:action => 'create'},     :html => {:multipart => true,:name => "touch"}) do |f| %>
<%= f.text_field :name,placeholder: 'title' %>
<%= f.text_field :display_name,placeholder: 'display_name' %>
<%= f.fields_for :images do |builder| %>
    <%= render "image", :f => builder %>
  <% end %>
<%= f.submit "Upload" %>
<% end %>
