<%= form_for(:image, :url => {:action => 'create'},     :html => {:multipart => true,:name => "touch"}) do |f| %>
<%= f.file_field :image %>
<%= f.text_field :title,placeholder: 'Ad title' %>
<%= f.fields_for :questions do |builder| %>
    <%= render "question", :f => builder %>
  <% end %>
<%= f.submit "Upload" %>
<% end %>
